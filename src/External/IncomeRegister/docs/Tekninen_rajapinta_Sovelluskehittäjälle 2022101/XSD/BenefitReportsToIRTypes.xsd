		<xs:sequence>
			<xs:element name="Type" type="xs:int" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Code" type="irct:String20" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
